<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="com.youthen.master.persistence.entity">
    <class name="Room" table="MST_ROOM">
         <id name="id"  column="ID"  type="java.lang.Long">
           <generator class="sequence">
				<param name="sequence">MST_ROOM</param>
			</generator>
        </id>
		<property name="roomName" column="ROOM_NAME" type="java.lang.String" />
		<property name="roomCode" column="ROOM_CODE" type="java.lang.String" />
		
	</class>
</hibernate-mapping>