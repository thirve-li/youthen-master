<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.youthen.master.persistence.entity">
	<class name="UserRole" table="T_USER_ROLE">
	    <id name="id" column="ID" type="java.lang.Long">
		    <generator class="identity"/>
		</id>
		<property name="userId" column="USER_ID" type="java.lang.String" />
		<property name="roleId" column="ROLE_ID" type="java.lang.Long" />
		<property name="companyId" column="COMPANY_ID" type="java.lang.Long" />
		<property name="deptId" column="DEPT_ID" type="java.lang.Long" />
		<property name="sysId" column="SYS_ID" type="java.lang.Long" />
	</class>

</hibernate-mapping>
